<odoo>
    <record id="view_account_asset_asset_form_magnus" model="ir.ui.view">
        <field name="model">account.asset.asset</field>
        <field name="inherit_id" ref="account_asset.view_account_asset_asset_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='value_residual']" position="after">
                <field name="book_value" widget="monetary"/>
            </xpath>
            <xpath expr="//field[@name='value_residual']" position="attributes">
                <attribute name="string">To Be Deprecated</attribute>
            </xpath>
        </field>
    </record>
</odoo>