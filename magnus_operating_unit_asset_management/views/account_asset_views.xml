<odoo>
    <record id="view_account_asset_form_magnus" model="ir.ui.view">
        <field name="model">account.asset</field>
        <field name="inherit_id" ref="account_asset_management.account_asset_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='account_analytic_id']" position="after">
                <field name="operating_unit_id"/>
            </xpath>
        </field>
    </record>
    <record id="view_asset_depreciation_confirmation_wizard_magnus" model="ir.ui.view">
        <field name="model">asset.depreciation.confirmation.wizard</field>
        <field name="inherit_id" ref="account_asset_management.view_asset_depreciation_confirmation_wizard"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='period_id']" position="after">
                <field name="operating_unit_id"/>
            </xpath>
        </field>
    </record>
</odoo>